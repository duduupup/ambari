<?xml version="1.0" encoding="UTF-8" ?>

<configuration support_final="true">

    <property>
        <name>cm_host</name>
        <description>CDH Manager ip</description>
        <display-name>cm.host</display-name>
        <value>{{cm_host}}</value>
    </property>

    <property>
        <name>cm_port</name>
        <description>CDH Manager Listen Port</description>
        <display-name>cm.port</display-name>
        <value>7180</value>
    </property>

    <property>
        <name>cm_user</name>
        <description>CDH Manager Read-only User</description>
        <display-name>cm.user</display-name>
        <value>view</value>
    </property>

    <property require-input="true">
        <name>cm_user_password</name>
        <value>view</value>
        <property-type>PASSWORD</property-type>
        <display-name>CDH Manager Read-only User Password</display-name>
        <value-attributes>
            <overridable>true</overridable>
            <type>password</type>
        </value-attributes>
    </property>

